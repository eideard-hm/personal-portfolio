---
const icons: Record<Tech, any> = {
  '.NET': (await import('~icons/logos/dotnet?raw')).default,
  'C#': (await import('~icons/logos/c-sharp?raw')).default,
  'Next.js': (await import('~icons/logos/nextjs-icon?raw')).default,
  'Node.js': (await import('~icons/logos/nodejs-icon?raw')).default,
  'Sql Server': (await import('~icons/simple-icons/microsoftsqlserver?raw'))
    .default,
  Angular: (await import('~icons/logos/angular-icon?raw')).default,
  Astro: (await import('~icons/logos/astro-icon?raw')).default,
  Bootstrap: (await import('~icons/logos/bootstrap?raw')).default,
  Composer: (await import('~icons/logos/composer?raw')).default,
  Git: (await import('~icons/logos/git-icon?raw')).default,
  GitHub: (await import('~icons/logos/github-icon?raw')).default,
  Go: (await import('~icons/logos/go?raw')).default,
  GraphQL: (await import('~icons/logos/graphql?raw')).default,
  Ionic: (await import('~icons/logos/ionic-icon?raw')).default,
  JavaScript: (await import('~icons/logos/javascript?raw')).default,
  MongoDB: (await import('~icons/logos/mongodb-icon?raw')).default,
  MySQL: (await import('~icons/logos/mysql?raw')).default,
  Netlify: (await import('~icons/logos/netlify-icon?raw')).default,
  PHP: (await import('~icons/logos/php?raw')).default,
  Qwik: (await import('~icons/logos/qwik-icon?raw')).default,
  React: (await import('~icons/logos/react?raw')).default,
  Typescript: (await import('~icons/logos/typescript-icon?raw')).default,
  Vite: (await import('~icons/logos/vitejs?raw')).default,
};
interface Props {
  name: Tech;
}

const { name } = Astro.props;
---

<Fragment set:html={icons[name as Tech]} />
