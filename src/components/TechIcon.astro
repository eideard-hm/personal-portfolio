---
const icons: Record<Tech, any> = {
  '.NET': (await import('~icons/simple-icons/dotnet?raw')).default,
  'C#': (await import('~icons/simple-icons/csharp?raw')).default,
  'Next.js': (await import('~icons/simple-icons/nextdotjs?raw')).default,
  'Node.js': (await import('~icons/simple-icons/nodedotjs?raw')).default,
  'Sql Server': (await import('~icons/simple-icons/microsoftsqlserver?raw'))
    .default,
  Angular: (await import('~icons/simple-icons/angular?raw')).default,
  Astro: (await import('~icons/simple-icons/astro?raw')).default,
  Git: (await import('~icons/simple-icons/git?raw')).default,
  Go: (await import('~icons/simple-icons/go?raw')).default,
  Javascript: (await import('~icons/simple-icons/javascript?raw')).default,
  MongoDB: (await import('~icons/simple-icons/mongodb?raw')).default,
  Qwik: (await import('~icons/logos/qwik-icon?raw')).default,
  React: (await import('~icons/simple-icons/react?raw')).default,
  Typescript: (await import('~icons/simple-icons/typescript?raw')).default,
};

interface Props {
  name: Tech;
}

const { name } = Astro.props;
---

<Fragment set:html={icons[name as Tech]} />
